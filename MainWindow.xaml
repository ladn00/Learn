<Window x:Class="Learndb.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Learndb"
        mc:Ignorable="d"
        Title="Главное окно" Height="450" Width="800" Style="{StaticResource WindowIcon}">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Frame Grid.Row="1" Name="frame" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" Click="btBack" Width="200" Height="30"/>
    </Grid>
</Window>
